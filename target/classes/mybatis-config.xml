<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
			<typeAlias type="com.spring.hotel.vo.HotelMemberVO" alias="Member"/>
			<typeAlias type="com.spring.hotel.vo.QuestionListVO" alias="Question"/>
			<typeAlias type="com.spring.hotel.vo.ReplyVO" alias="Reply"/>
			<typeAlias type="com.spring.hotel.vo.RoomVO" alias="Room"/>
			<typeAlias type="com.spring.hotel.vo.BookingVO" alias="Booking"/>
	</typeAliases>
	
	<mappers>
			<mapper resource="mappers/memberMapper.xml"/>
			<mapper resource="mappers/questionMapper.xml"/>
			<mapper resource="mappers/bookingMapper.xml"/>
			
	</mappers>
</configuration>